{
  "name": "Alarm teste",
  "version": "1.7",
  "manifest_version": 2,
  "description": "Tired of taking the Nerd Bait? Use GawkBlocker!",
  "background": {
    "scripts": ["eventPage.js"],
	"persistent": false	
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "http://*/*", "https://*/*", "background", "alarms"] 
}